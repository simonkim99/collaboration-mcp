{
  "name": "collaboration-mcp",
  "version": "0.1.0",
  "description": "MCP server for managing multiple AI services via shell commands",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "collaboration-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "services"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.5",
    "jsdom": "^27.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.5"
  }
}
